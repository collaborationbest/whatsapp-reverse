<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="100.0dip" />
            <solid android:color="#ff364147" />
            <stroke android:width="1.0dip" android:color="#ff364147" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="100.0dip" />
            <solid android:color="#ff4d565c" />
            <stroke android:width="1.0dip" android:color="#ff4d565c" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="100.0dip" />
            <solid android:color="#ff202c33" />
            <stroke android:width="1.0dip" android:color="#ff202c33" />
        </shape>
    </item>
</selector>